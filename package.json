{
  "name": "upload-amazon-appstore",
  "version": "1.0",
  "private": true,
  "description": "An action to upload a signed Android release (.apk) to the Amazon Appstore",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/galonga/upload-amazon-appstore"
  },
  "keywords": [
    "actions",
    "appstore",
    "amazon"
  ],
  "author": "galonga",
  "bugs": {
    "url": "https://github.com/galonga/upload-amazon-appstore/issues"
  },
  "homepage": "https://github.com/galonga/upload-amazon-appstore#readme",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.6.0",
    "@actions/github": "5.1.1",
    "node-fetch": "3.2.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/parser": "^5.49.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^8.27.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.6.0",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.2.3",
    "prettier": "2.7.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.4.4"
  }
}
